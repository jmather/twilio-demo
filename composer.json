{
    "autoload":{
        "psr-0":{
            "":"src/"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "majax/twilio",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/jmather/MajaxTwilio.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": [ "lib/Majax/" ]
                }
            }
        }
    ],
    "require":{
        "twilio/sdk":"dev-master",
        "silex/silex": "dev-master",
        "majax/twilio": "dev-master",
        "monolog/monolog": ">=1.0.0"
    },
    "require-dev":{
        "phpunit/phpunit":"3.7.*",
        "symfony/browser-kit": "2.1.*"
    },
    "config":{
        "bin-dir":"bin"
    }
}
